   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 46,
   "metadata": {},
   "outputs": [],
   "source": [
    "df = pd.read_csv('../data/all_euvm.csv')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 149,
   "metadata": {},
   "outputs": [],
   "source": [
    "exorb = 1700\n",
    "norbs = 50\n",
    "iocond = df['inout'] == 'i'\n",
    "orbcond = (df['orbit'] > exorb-norbs/2) & (df['orbit']<exorb+norbs/2)\n",
    "\n",
    "tg = df[iocond&orbcond].copy()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 150,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "plt.figure()\n",
    "for orbno, prof in tg.groupby('orbit'):\n",
    "    plt.scatter(prof['temp'],prof['alt_temp'],s=5)\n",
    "#plt.errorbar(agg['temp_mean'],agg['alt_mean'],xerr=agg['temp_std'])\n",
    "plt.xlim(80,300)\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 151,
   "metadata": {},
   "outputs": [],
   "source": [
    "bins = np.arange(100,200,3)\n",
    "mids_y =  (bins[0:-1] + bins[1:])/2 # midpoints\n",
    "binalt = pd.cut(tg['alt_temp'],bins,labels=mids_y)\n",
    "tg['alt_temp_bin'] = binalt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 152,
   "metadata": {},
   "outputs": [],
   "source": [
    "bin_mean = tg.groupby('alt_temp_bin').mean()[['alt_temp','temp']].reset_index()\n",
    "bin_std = tg.groupby('alt_temp_bin').std()[['temp']].reset_index()\n",
    "bin_count = tg.groupby('alt_temp_bin').count()[['temp']].reset_index()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 153,
   "metadata": {},
   "outputs": [],
   "source": [
    "bin_mean.rename(columns={'alt_temp':'alt_mean','temp':'temp_mean'},inplace=True)\n",
    "bin_std.rename(columns={'temp':'temp_std'},inplace=True)\n",
    "bin_count.rename(columns={'temp':'temp_count'},inplace=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 154,
   "metadata": {},
   "outputs": [],
   "source": [
    "agg = pd.concat([bin_mean,bin_std,bin_count],axis=1)"
   ]
  },
